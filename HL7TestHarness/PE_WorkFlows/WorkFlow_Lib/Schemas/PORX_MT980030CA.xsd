                                 maximumMultiplicity="1"
                                 conformance="R"
                                 isMandatory="true"
                                 isStructural="false">
                     <mif:businessName name="B:Dosage Precondition Value"/>
                     <mif:annotations>
                        <mif:definition>
                           <mif:text>
                              <mif:p>A specific value or range of values of the Dosage Precondition Type, for which the recommended dosage applies.</mif:p>
                              <mif:p>This includes min-max age range, min-max weights, etc.</mif:p>
                              <mif:p>This is a mandatory attribute as the specific range of values must be known.</mif:p>
                           </mif:text>
                        </mif:definition>
                        <mif:usageNotes>
                           <mif:text>
                              <mif:p>If not specified, it means that the range is based on a criteria (e.g. weight), but the specific range on which the criteria is based is not known.</mif:p>
                           </mif:text>
                        </mif:usageNotes>
                        <mif:rationale>
                           <mif:text>
                              <mif:p>Allows direct comparison of the patient's characteristics with the minimum and maximum values specified.</mif:p>
                              <mif:p>The element is mandatory because there's no point in identifying that the dosage range is based on criteria unless the specific criterion used is expressed.</mif:p>
                           </mif:text>
                        </mif:rationale>
                        <mif:mapping sourceName="PEI">
                           <mif:text>
                              <mif:p>Minimum Age</mif:p>
                           </mif:text>
                        </mif:mapping>
                        <mif:mapping sourceName="PEI">
                           <mif:text>
                              <mif:p>Maximum Age</mif:p>
                           </mif:text>
                        </mif:mapping>
                     </mif:annotations>
