            <mif:class xmlns:mif="urn:hl7-org:v3/mif" name="SubstanceAdministrationEventCriterion"
                       isAbstract="false">
               <mif:businessName name="Recommended Dosage"/>
               <mif:derivationSupplier staticModelDerivationId="1" className="SubstanceAdministration"/>
               <mif:graphicRepresentation presentation="HL7">
                  <mif:graphElement shapeId="Sheet.76" containerDiagramName="Main" shapeTemplate="Act"
                                    isWidthAutoSize="true"
                                    isHeightAutoSize="true">
                     <mif:position x="10.190" y="4.170"/>
                     <mif:size width="3.479" height="0.781"/>
                  </mif:graphElement>
               </mif:graphicRepresentation>
               <mif:annotations>
                  <mif:definition>
                     <mif:text>
                        <mif:p>Identifies the outer dosage boundaries that were exceeded and triggered the issue. Included in this information are the drug-specific recommended dosage ranges for various age groups and weight classes.</mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <mif:p>Provides a reference for calculating optimum dose of medication for patients.</mif:p>
                     </mif:text>
                  </mif:rationale>
               </mif:annotations>
            </mif:class>
         </xs:appinfo>
      </xs:annotation>
