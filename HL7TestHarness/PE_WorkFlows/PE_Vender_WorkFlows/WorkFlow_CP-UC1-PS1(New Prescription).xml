<workflows testDataFile="TestData.xml">
    <workflow id="w_CP-UC1-PS1-NEW" description="Workflow CP-UC1-PS1(New Prescription)">

        <reference filename="..\SubWorkFlows\SubWorkFlow_Manage_Last_Issue.xml">
            <paramList name="ProfileData">
                <param name="profile-description" value="Staged Profile"/>
                <param name="profile-sequence" value="(Generated)"/>
            </paramList>
            <attribute xpath="/workflows[1]/workflow[1]/message[1]" name="optional" value="true" force="true"/>
            <attribute xpath="/workflows[1]/workflow[1]/message[1]" name="globally-non-sequential" value="true" force="true"/>
            <attribute xpath="/workflows[1]/workflow[1]/message[1]" name="repeatable" value="true" force="true"/>
            <attribute xpath="/workflows[1]/workflow[1]/message[1]/request[1]" name="simulator" value="false" force="true"/>
        </reference>
        
        <reference filename="..\SubWorkFlows\SubWorkFlow_Find_Candidate.xml">
            <paramList name="FindCandidate">
                <param name="patient-description" value="Patient 3"/>
                <param name="by-family-name" value="true"/>
            </paramList>
            <replace from="TESTNAME" to="CP-UC1-PS1(New Prescription) Find Candidate"/>
        </reference>

        <reference filename="..\SubWorkFlows\SubWorkFlow_Patient_Profile.xml">
            <paramList name="QueryList">
                <param name="profile-description" value="Staged Profile"/>
                <param name="profile-sequence" value="(Generated)"/>
                <param name="author-description" value="Pharmacist 1"/>
                <param name="location-description" value="Pharmacy 1"/>
                <param name="patient-description" value="Patient 3"/>
            </paramList>
            <replace from="TESTNAME" to="CP-UC1-PS1(New Prescription) Patient Profile"/>
        </reference>

        <reference filename="..\SubWorkFlows\SubWorkFlow_Add_Prescription.xml">
            <paramList name="ProfileData">
                <param name="profile-description" value="Staged Profile"/>
                <param name="profile-sequence" value="New Prescription v1"/>
                <param name="prescription-description" value="Prescription 1"/>
            </paramList>
        </reference>

        <reference filename="..\SubWorkFlows\SubWorkFlow_Patient_Profile(Prescription Detail).xml">
            <paramList name="QueryList">
                <param name="profile-description" value="Staged Profile"/>
                <param name="profile-sequence" value="(Generated)"/>
                <param name="author-description" value="Pharmacist 1"/>
                <param name="location-description" value="Pharmacy 1"/>
                <param name="patient-description" value="Patient 3"/>
            </paramList>
            <attribute xpath="/workflows[1]/workflow[1]/message[1]/request[1]" name="simulator" value="false" force="true"/>
        </reference>
        
        <reference filename="..\SubWorkFlows\SubWorkFlow_Add_Dispense.xml">
            <paramList name="ProfileData">
                <param name="profile-description" value="Staged Profile"/>
                <param name="profile-sequence" value="New Prescription v1"/>
                <param name="dispense-description" value="Dispense 1 of Prescription 1"/>
            </paramList>
        </reference>

        <reference filename="..\SubWorkFlows\SubWorkFlow_Patient_Profile(Dispense Detail).xml">
            <paramList name="QueryList">
                <param name="profile-description" value="Staged Profile"/>
                <param name="profile-sequence" value="(Generated)"/>
                <param name="author-description" value="Pharmacist 1"/>
                <param name="location-description" value="Pharmacy 1"/>
                <param name="patient-description" value="Patient 3"/>
            </paramList>
            <attribute xpath="/workflows[1]/workflow[1]/message[1]/request[1]" name="simulator" value="false" force="true"/>
        </reference>
        
