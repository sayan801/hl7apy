                        <xs:choice maxOccurs="unbounded">
                            <xs:any/>
                        </xs:choice>
                        <xs:attribute name="description" use="required"/>
                        <xs:attribute name="sequence" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="patients">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="patient"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="patient">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="client"/>
                <xs:element ref="server"/>
            </xs:sequence>
            <xs:attribute name="OID" use="required" type="xs:NCName"/>
            <xs:attribute name="age" use="required" type="xs:NMTOKEN"/>
            <xs:attribute name="description" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="providers">
        <xs:complexType>
